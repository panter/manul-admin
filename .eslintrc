{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "globalReturn": true,
      "jsx": true
    }
  },
   "globals": {
    "document": true,
    "window": true
  },
  "plugins": [
    "meteor",
    "react"
  ],
  "extends": [
    "airbnb",
    "plugin:meteor/recommended",
    "plugin:react/recommended",
    "prettier"
  ],
  "rules": {
    "global-require": 0,
    "arrow-body-style": [1, "as-needed"],
    "no-unused-vars": 1,
    "no-underscore-dangle": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": 0,
    "consistent-return": 0,
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0
  }
}
